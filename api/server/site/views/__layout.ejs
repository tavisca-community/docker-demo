    <style>
      html, body {
        height: 100%;
        font-size: 13px;
      }
      #wrapper {
        min-height: 100%;
        position: relative;
      }
      .navbar-brand {
        background-image: url("https://d3oypxn00j2a10.cloudfront.net/0.16.0/img/nav/docker-logo-loggedout.png");
        background-repeat: no-repeat;
        background-position: 10% 0;
        display: block;
        height: 40px;
        margin: 15px 0 20px;
        padding: 0;
        width: 100%;
      }

      .navbar-brand a {
        display: block;
        margin-top: 0;
        width: 400px;
        min-height: 52px;
        overflow: hidden;
      }
      .footer {
          position: absolute;
          left: 0;
          bottom: 0;
          width: 100%;
      }
    </style>
  </head>
  <body style="background-color: #eee;">
    <div class="wrapper">
      <div class='header'>
        <nav class='navbar' role='navigation' style="margin-bottom:0; background: #fff;">
